    <!-- Suppress vulnerabilities in test-scoped dependencies -->
    <suppress>
        <notes>Test dependencies are not included in production builds</notes>
        <packageUrl regex="true">^pkg:maven/org\.testcontainers/.*$</packageUrl>
        <cpe>cpe:/a:*:*</cpe>
    </suppress>
</suppressions>
